{

    "builds": [{

        "src": "myweb/myweb/wsgi.py",

        "use": "@ardnt/vercel-python-wsgi",

        "config": { "maxLambdaSize": "15mb" ,
        "runtime": "python3.6"}

    }],

    "routes": [

        {

            "src": "/(.*)",

            "dest": "myweb/myweb/wsgi.py"
        }

    ]

}